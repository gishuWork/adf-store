{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "gishuKaKarkhana"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/pipeline1')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy data1",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "DelimitedTextSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings"
								}
							},
							"sink": {
								"type": "AzureSqlSink",
								"tableOption": "autoCreate"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "MovieId",
											"type": "Int32"
										},
										"sink": {
											"name": "MovieId",
											"type": "String"
										}
									},
									{
										"source": {
											"name": " Title",
											"type": "String"
										},
										"sink": {
											"name": " Title",
											"type": "String"
										}
									},
									{
										"source": {
											"name": " Cast",
											"type": "String"
										},
										"sink": {
											"name": " Cast",
											"type": "String"
										}
									},
									{
										"source": {
											"name": " Type",
											"type": "String"
										},
										"sink": {
											"name": " Type",
											"type": "String"
										}
									},
									{
										"source": {
											"name": " Price",
											"type": "Decimal"
										},
										"sink": {
											"name": " Price",
											"type": "String"
										}
									}
								]
							}
						},
						"inputs": [
							{
								"referenceName": "myCsvData",
								"type": "DatasetReference"
							}
						],
						"outputs": [
							{
								"referenceName": "mySqlData",
								"type": "DatasetReference"
							}
						]
					}
				],
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/myCsvData')]",
				"[concat(variables('factoryId'), '/datasets/mySqlData')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/myCsvData')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_blob_store",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"fileName": "Movies.csv",
						"container": "peti"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": [
					{
						"name": "MovieId",
						"type": "String"
					},
					{
						"name": " Title",
						"type": "String"
					},
					{
						"name": " Cast",
						"type": "String"
					},
					{
						"name": " Type",
						"type": "String"
					},
					{
						"name": " Price",
						"type": "String"
					}
				]
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/mySqlData')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ls_azure_sql",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureSqlTable",
				"schema": [],
				"typeProperties": {
					"schema": "dbo",
					"table": "adfsink"
				}
			},
			"dependsOn": []
		}
	]
}